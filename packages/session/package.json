{
  "name": "@svelte-dev/session",
  "version": "1.0.0",
  "description": "Simple Session Storage Management for Svelte",
  "author": "Willin Wang <willin@willin.org> (https://willin.wang/)",
  "keywords": [
    "svelte",
    "cookie",
    "session",
    "cache",
    "store",
    "strategies"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willin/svelte-turbo.git"
  },
  "bugs": {
    "url": "https://github.com/willin/svelte-turbo/issues"
  },
  "homepage": "https://svelte.js.cool",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "echo vite dev",
    "build": "vite build && npm run package",
    "dev:docs": "typedoc --watch",
    "build:docs": "typedoc",
    "lint": "prettier --write . && eslint . --fix",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "LICENSE",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0 || ^4.0.0"
  },
  "optionalDependencies": {
    "@cloudflare/workers-types": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@miniflare/kv": "^2.14.1",
    "@miniflare/storage-memory": "^2.14.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.4",
    "@sveltejs/package": "^2.0.0",
    "@svelte-dev/eslint-config": "*",
    "eslint": "^8.28.0",
    "publint": "^0.1.9",
    "svelte": "^5.0.0-next.22",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typedoc-plugin-extras": "^3.0.0",
    "typedoc-plugin-google-ads": "^1.6.0",
    "typedoc-plugin-keywords": "^1.6.0",
    "typedocs": "^0.6.7",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.34.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
