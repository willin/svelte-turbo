{
  "name": "@svelte-dev/auth-sso",
  "version": "2.0.0",
  "description": "SSO for Svelte Auth",
  "author": "Willin Wang <willin@willin.org> (https://willin.wang/)",
  "keywords": [
    "svelte",
    "auth",
    "authentication",
    "local",
    "authing",
    "oauth2",
    "sso",
    "strategies"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willin/svelte-turbo.git"
  },
  "bugs": {
    "url": "https://github.com/willin/svelte-turbo/issues"
  },
  "homepage": "https://svelte.js.cool",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "echo vite dev",
    "build": "vite build && npm run package",
    "lint": "prettier --write . && eslint . --fix",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "LICENSE",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "dependencies": {
    "@svelte-dev/auth-oauth2": "*"
  },
  "peerDependencies": {
    "@svelte-dev/auth": "*"
  },
  "devDependencies": {
    "@svelte-dev/eslint-config": "*",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.4",
    "@sveltejs/package": "^2.0.0",
    "eslint": "^8.28.0",
    "publint": "^0.1.9",
    "svelte": "^5.0.0-next.22",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.34.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
